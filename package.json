{
  "name": "@autorest/az",
  "version": "1.7.3",
  "description": "Autorest Azure AZ extension",
  "main": "dist/src/index.js",
  "engines": {
    "node": ">=10.12.0"
  },
  "scripts": {
    "start": "node dist/src/index.js",
    "postinstall": "mkdirp dist && cpy src/python/* dist/ --parents && tsc dist/src/python/setup.ts && node dist/src/python/setup.js dist/src/python/install.py",
    "debug": "node --max_old_space_size=4096 --inspect-brk ./dist/src/index.js",
    "eslint-fix": "eslint . --ext .ts --ignore-pattern node_modules/ --ignore-pattern dist/ --fix",
    "eslint": "eslint . --ext .ts --ignore-pattern node_modules/ --ignore-pattern dist/ --quiet",
    "build": "npm run eslint && tsc",
    "postbuild": "cpy templates ../dist/src --cwd=src/ --parents && cpy python/install.py python/venvtools.py python/requirements.txt ../dist/src --cwd=src/ --parents",
    "start-testserver": "start-autorest-testserver",
    "stop-testserver": "stop-autorest-testserver",
    "watch": "tsc -p . --watch",
    "prepare": "npm run build",
    "test": "npm run build && mocha dist/test --recursive",
    "test-report": "npm run build && nyc mocha dist/test --recursive",
    "clean": "ver > nul 2>&1 nul && npm run --silent clean-cmd || npm run --silent clean-bash",
    "clean-cmd": "if exist dist rmdir /s /q dist && exit 0 || exit 0 ",
    "clean-bash": "rm nul && rm -rf dist && exit 0 || exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/autorest.az.git"
  },
  "keywords": [
    "autorest",
    "extension"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "devDependencies": {
    "@microsoft.azure/autorest.testserver": "^2.10.45",
    "@types/chai": "^4.2.7",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.0",
    "bufferutil": "^4.0.1",
    "chai": "^4.2.0",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.17",
    "node-yaml": "^3.2.0",
    "nyc": "^15.0.0",
    "source-map-support": "^0.5.13",
    "typescript": "^3.7.5",
    "utf-8-validate": "^5.0.2"
  },
  "dependencies": {
    "@azure-tools/async-io": "3.0.209",
    "@autorest/extension-base": "3.2.0",
    "@azure-tools/codegen": "2.5.276",
    "@azure-tools/codemodel": "4.13.325",
    "@azure-tools/extension": "3.1.269",
    "@azure-tools/linq": "3.1.232",
    "autorest": "3.0.6336",
    "await-exec": "^0.1.2",
    "compare-versions": "^3.6.0",
    "dir-compare": "^2.2.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "js-yaml": "^3.13.1",
    "jszip-sync": "^3.2.1-sync",
    "node-yaml": "^3.2.0",
    "nunjucks": "^3.2.2",
    "oav": "2.2.0",
    "prettier": "^2.2.1",
    "request": "^2.87.0",
    "request-promise-native": "1.0.8",
    "@types/mocha": "^5.2.5",
    "@types/node": "10.17.0",
    "mkdirp": "1.0.4",
    "cpy-cli": "^2.0.0",
    "del-cli": "^3.0.0"
  },
  "files": [
    "dist/src",
    "dist/test"
  ]
}
