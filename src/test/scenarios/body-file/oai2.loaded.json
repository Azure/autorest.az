{
 "swagger": "2.0",
 "info": {
  "title": "AutoRest Swagger BAT File Service",
  "description": "Test Infrastructure for AutoRest Swagger BAT",
  "version": "1.0.0"
 },
 "host": "localhost:3000",
 "schemes": [
  "http"
 ],
 "produces": [
  "application/json"
 ],
 "consumes": [
  "application/json"
 ],
 "paths": {
  "/files/stream/nonempty": {
   "get": {
    "operationId": "files_GetFile",
    "description": "Get file",
    "produces": [
     "image/png",
     "application/json"
    ],
    "tags": [
     "Files"
    ],
    "responses": {
     "200": {
      "description": "The PNG file",
      "schema": {
       "type": "file"
      }
     },
     "default": {
      "description": "Unexpected error",
      "schema": {
       "$ref": "http://localhost:3000/__files/body-file.json#/definitions/Error"
      }
     }
    }
   }
  },
  "/files/stream/verylarge": {
   "get": {
    "operationId": "files_GetFileLarge",
    "description": "Get a large file",
    "produces": [
     "image/png",
     "application/json"
    ],
    "tags": [
     "Files"
    ],
    "responses": {
     "200": {
      "description": "The large file",
      "schema": {
       "type": "file"
      }
     },
     "default": {
      "description": "Unexpected error",
      "schema": {
       "$ref": "http://localhost:3000/__files/body-file.json#/definitions/Error"
      }
     }
    }
   }
  },
  "/files/stream/empty": {
   "get": {
    "operationId": "files_GetEmptyFile",
    "description": "Get empty file",
    "produces": [
     "image/png",
     "application/json"
    ],
    "tags": [
     "Files"
    ],
    "responses": {
     "200": {
      "description": "The empty file stream",
      "schema": {
       "type": "file"
      }
     },
     "default": {
      "description": "Unexpected error",
      "schema": {
       "$ref": "http://localhost:3000/__files/body-file.json#/definitions/Error"
      }
     }
    }
   }
  }
 },
 "definitions": {
  "Error": {
   "type": "object",
   "properties": {
    "status": {
     "type": "integer",
     "format": "int32"
    },
    "message": {
     "type": "string"
    }
   }
  }
 }
}